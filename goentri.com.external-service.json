{
    "providerId": "goentri.com",
    "providerName": "Entri",
    "serviceId": "external-service",
    "serviceName": "ExternalService",
    "version": 122,
    "logoUrl": "https://cdn.goentri.com/logo.svg",
    "description": "Allows user to easily set up domain using Entri",
    "shared": true,
    "sharedProviderName": true,
    "syncPubKeyDomain": "goentri.com",
    "syncRedirectDomain": "api.goentri.com, goentri.com, entri.com, app.entri.com,app.goentri.com",
    "records": [
        {
            "type": "CNAME",
            "groupId": "a1BigCartelProxy",
            "host": "www",
            "pointsTo": "%webSitePointsTo%",
            "ttl": "3600"
        },
        {
            "type": "REDIR301",
            "groupId": "a1BigCartelProxy",
            "host": "@",
            "target": "%target%"
        },
        {
            "type": "CNAME",
            "groupId": "a2BigCartelProxy",
            "host": "%subdomain%",
            "pointsTo": "%webSitePointsTo%",
            "ttl": "3600"
        },
        {
            "type": "CNAME",
            "groupId": "a1Lodgify",
            "host": "%CnameHost1Lodgify%",
            "pointsTo": "%CnameValue1Lodgify%",
            "ttl": "300"
        },
        {
            "type": "CNAME",
            "groupId": "a2Lodgify",
            "host": "%CnameHost2Lodgify%",
            "pointsTo": "%CnameValue2Lodgify%",
            "ttl": "300"
        },
        {
            "type": "TXT",
            "groupId": "a3Lodgify",
            "host": "%TxtHostLodgify%",
            "ttl": "300",
            "data": "%TxtValueLodgify%"
        },
        {
            "type": "A",
            "groupId": "a4Lodgify",
            "host": "%IPhost1Lodgify%",
            "pointsTo": "%IPvalueLodgify%",
            "ttl": "300"
        },
        {
            "type": "A",
            "groupId": "a5Lodgify",
            "host": "%IPhost2Lodgify%",
            "pointsTo": "%IPvalueLodgify%",
            "ttl": "300"
        },
        {
            "type": "CNAME",
            "groupId": "a1Swell",
            "host": "%cnameHostSwell%",
            "pointsTo": "%cnamePointsToSwell%",
            "ttl": "3600"
        },
        {
            "type": "CNAME",
            "groupId": "a2Swell",
            "host": "%cnameHostSwell2%",
            "pointsTo": "%cnamePointsToSwell%",
            "ttl": "3600"
        },
        {
            "type": "A",
            "groupId": "a1learndash",
            "host": "@",
            "pointsTo": "%pointstoIplearndash%",
            "ttl": "3600"
        },
        {
            "type": "CNAME",
            "groupId": "a2learndash",
            "host": "%cnameHost2learndash%",
            "pointsTo": "%dataValue2learndash%",
            "ttl": "3600"
        },
        {
            "type": "MX",
            "groupId": "a3learndash",
            "host": "@",
            "pointsTo": "%mxValuelearndash%",
            "ttl": "300",
            "priority": "10"
        },
        {
            "type": "MX",
            "groupId": "a4learndash",
            "host": "@",
            "pointsTo": "%mxValue2learndash%",
            "ttl": "300",
            "priority": "20"
        },
        {
            "type": "SPFM",
            "groupId": "a5learndash",
            "host": "@",
            "ttl": "3600",
            "spfRules": "%spfRuleslearndash%"
        },
        {
            "type": "CNAME",
            "groupId": "a6learndash",
            "host": "%hostValue6learndash%",
            "pointsTo": "%dataValue6learndash%",
            "ttl": "3600"
        },
        {
            "type": "CNAME",
            "groupId": "a7learndash",
            "host": "%hostValue7learndash%",
            "pointsTo": "%dataValue7learndash%",
            "ttl": "3600"
        },
        {
            "type": "TXT",
            "groupId": "a8learndash",
            "host": "%hostValue8learndash%",
            "ttl": "3600",
            "data": "%dataValue8learndash%"
        },
        {
            "type": "TXT",
            "groupId": "a9learndash",
            "host": "%hostValue9learndash%",
            "ttl": "3600",
            "data": "%dataValue9learndash%"
        }
    ]
}
